---
classes:
  -  foodity-common
  -  foodity-puppet
  -  foodity-ssh

foodity-ssh::username: 'root'
foodity-ssh::server_list:
  - git.foodity.com
  - github.com

ssh_git_remote: 'git@git.foodity.com:claudio.benfatto/puppet-automation.git'

aws_region: 'eu-west-1'

aws_access_key_id: ENC[PKCS7,]

aws_secret_access_key: ENC[PKCS7,]

private_ssh_key:
  '/root/.ssh/id_rsa':
    owner: 'root'
    mode:  '0600'
    content: ENC[PKCS7,] 

software:
  pv:
    ensure: 'latest'
  git:
   ensure: 'latest'
  python-pip: 
    ensure: 'latest'
  build-essential: 
    ensure: 'latest'
  checkinstall: 
    ensure: 'latest'
  lynx:
    ensure: 'latest'
  nmap:
    ensure: 'latest'
  dselect:
    ensure: 'latest'
  tree:
    ensure: 'latest'
  expect:
    ensure: 'latest'
  ruby1.9.1-dev:
    ensure: 'latest'
